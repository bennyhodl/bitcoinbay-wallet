{
  "expo": {
    "name": "Bay Wallet",
    "slug": "baywallet",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "splash": {
        "image": "./assets/logo.jpg",
        "resizeMode": "cover"
      },
      "supportsTablet": true,
      "bundleIdentifier": "io.bitcoinbay.baywallet",
      "buildNumber": "3"
    },
    "android": {
      "splash": {
        "mdpi": "./assets/logo.jpg",
        "hdpi": "./assets/logo.jpg",
        "xhdpi": "./assets/logo.jpg",
        "xxhdpi": "./assets/logo.jpg",
        "xxxhdpi": "./assets/logo.jpg",
        "resizeMode": "cover"
      },
      "package": "io.bitcoinbay.baywallet",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpg",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    }
  }
}
